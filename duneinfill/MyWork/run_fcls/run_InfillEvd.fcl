#include "services_dune.fcl"
#include "InfillEvdConfig.fcl"

process_name: InfillEvd

source:
{
  module_type: RootInput
  maxEvents:   1
}

services:
{
  @table::protodune_services
  TFileService: { fileName: "infill_evd.root" }
